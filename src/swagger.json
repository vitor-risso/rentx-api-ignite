{
  "openapi": "3.0.0",
  "info": {
    "title": "Rentx Docs",
    "description": "This is an api rent",
    "version": "1.0.0",
    "contact": {
      "email": "vitor@test.com"
    }
  },
  "paths": {
    "/categories": {
      "post": {
        "tags": [
          "Categories"
        ],
        "summary": "Create a category",
        "description": "Create a new category",
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "name": {
                    "type": "string"
                  },
                  "description": {
                    "type": "string"
                  }
                },
                "example": {
                  "name": "category name example",
                  "description": "Category description example"
                }
              }
            }
          }
        },
        "responses": {
          "201": {
            "description": "Created"
          },
          "500": {
            "description": "Internal server error"
          }
        }
      },
      "get": {
        "tags": [
          "Categories"
        ],
        "summary": "List all categories",
        "description": "List all categories",
        "responses": {
          "200": {
            "description": "Success",
            "content": {
              "application/json": {
                "type": "array",
                "items": {
                  "type": "object",
                  "properties": {
                    "name": {
                      "type": "string"
                    },
                    "description": {
                      "type": "string"
                    }
                  }
                }
              }
            }
          }
        }
      }
    },
    "/categories/import": {
      "post": {
        "tags": [
          "Categories"
        ],
        "summary": "Upload a new category",
        "description": "Upload a new category",
        "requestBody": {
          "content": {
            "multipart/form-data": {
              "schema": {
                "type": "object",
                "properties":{
                  "file":{
                    "type":"string",
                    "format": "binary"
                  }
                }
              }
            }
          }
        },
        "responses":{
          "201":{
            "description":"Created"
          }
        }
      }
    },
    "/specificatons": {
      "post": {
        "tags": [
          "Specifications"
        ],
        "summary": "Create specification",
        "description": "Create a new specification",
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/definitions/Specifications"
              }
            }
          }
        },
        "responses": {
          "201": {
            "description": "Created"
          },
          "500": {
            "description": "Specification already exists"
          }
        }
      }
    }
  },
  "definitions": {
    "Specifications": {
      "type": "object",
      "properties": {
        "name": {
          "type": "string"
        },
        "description": {
          "type": "string"
        }
      }
    }
  }
}
